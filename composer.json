{
  "name": "arturu/laying",
  "description": "Laying convert YAML to XML/HTML file",
  "type": "library",
  "keywords": [
    "XML generator",
    "HTML generator",
    "Template generator",
    "Template automation",
    "YML"
  ],
  "license": "GPL v3.0",
  "homepage": "http://arturu.it",
  "authors": [
    {
      "name": "Pietro Arturo Panetta",
      "email": "arturu@arturu.it"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "ext-tidy": "*",
    "symfony/yaml": ">=2.8",
    "symfony/console": ">=2.8",
    "symfony/process": ">=2.8",
    "symfony/filesystem": ">=2.8",
    "arturu/xml-tag": ">=1.3"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.7",
    "phpunit/php-invoker" : "~1.0"
  },
  "bin": [
    "bin/console"
  ],
  "autoload": {
    "psr-4": {
      "Arturu\\Laying\\": "src"
    }
  }
}
